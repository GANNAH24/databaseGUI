@model MS3GUI.Models.Assessment

<h1>Details</h1>

<div>
    <h4>Assessment</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">Type</dt>
        <dd class="col-sm-10">@Model.Atype</dd>
        <dt class="col-sm-2">Total Marks</dt>
        <dd class="col-sm-10">@Model.TotalMarks</dd>
        <dt class="col-sm-2">Passing Marks</dt>
        <dd class="col-sm-10">@Model.PassingMarks</dd>
        <dt class="col-sm-2">Criteria</dt>
        <dd class="col-sm-10">@Model.Criteria</dd>
        <dt class="col-sm-2">Weightage</dt>
        <dd class="col-sm-10">@Model.Weightage</dd>
        <dt class="col-sm-2">Description</dt>
        <dd class="col-sm-10">@Model.Adescription</dd>
        <dt class="col-sm-2">Title</dt>
        <dd class="col-sm-10">@Model.Title</dd>
        <dt class="col-sm-2">Module</dt>
        <dd class="col-sm-10">@Model.Module?.Title</dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.Id">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
