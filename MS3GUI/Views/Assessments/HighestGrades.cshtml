




@model IEnumerable<HighestGrade>

<h2>Highest Grades</h2>

<table class="table">
    <thead>
        <tr>
            <th>Course ID</th>
            <th>Assessment ID</th>
            <th>Title</th>
            <th>Highest Max Points</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var grade in Model)
        {
            <tr>
                <td>@grade.CourseID</td>
                <td>@grade.ID</td>
                <td>@grade.Title</td>
                <td>@grade.HighestMaxPoints</td>
            </tr>
        }
    </tbody>
</table>
