@model MS3GUI.Models.TakenAssessment

<h1>Create</h1>

<h4>Taken Assessment</h4>
<hr />
<div class="row">
    <d class="col-md-4">
        <form asp-action="Create">
            <div class="form-group">
                <label asp-for="AssessmentId" class="control-label"></label>
                <select asp-for="AssessmentId" class="form-control" asp-items="ViewBag.AssessmentId"></select>
                <span asp-validation-for="AssessmentId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="LearnerId" class="control-label"></label>
                <select asp-for="LearnerId" class="form-control" asp-items="ViewBag.LearnerId"></select>
                <span asp-validation-for="LearnerId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Score" class="control-label"></label>
                <input asp-for="Score" class="form-control" />
                <span asp-validation-for="Score" class="text-danger"></span>
            </div>


            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
</div>
</div>


